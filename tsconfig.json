    "jsx": "react",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5",
    "types": [
      "vitest/globals"
    ]
  },
  "exclude": [
    "node_modules",
    "public"
  ]
}