    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  }
