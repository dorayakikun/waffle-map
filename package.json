    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/typescript-estree": "^5.0.0",
    "babel-loader": "^8.0.5",
    "connect": "^3.6.6",
    "core-js": "^3.0.0",
    "css-loader": "^6.0.0",
    "eslint": "^8.0.0",
