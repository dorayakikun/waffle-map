{
  "name": "waffle-map",
  "version": "1.21.0",
  "description": "Fantastic mesh renderer",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "build-storybook": "build-storybook",
    "dev": "webpack-dev-server --hot --mode development",
    "fmt": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "pages": "gh-pages -d public -m 'Auto publish'",
    "regression": "build-storybook && yarn screenshot && reg-suit sync-expected && reg-suit run",
    "screenshot": "node screenshot.js",
    "storybook": "start-storybook -p 6006 -c .storybook -s public",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit"
