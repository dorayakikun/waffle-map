{
  "name": "waffle-map",
  "version": "1.21.0",
  "description": "Fantastic mesh renderer",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "start": "NODE_OPTIONS='--openssl-legacy-provider' webpack serve --hot --mode development",
    "fmt": "deno fmt -c deno.jsonc",
    "lint": "deno lint -c deno.jsonc",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit"
  },
  "author": "dorayakikun <github@dorayakikun.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map.git"
  },
  "homepage": "https://dorayakikun.github.io/waffle-map/",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@emotion/jest": "^11.11.0",
    "@types/jest": "^29.5.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-loader": "^9.1.0",
    "connect": "^3.7.0",
    "core-js": "^3.33.0",
    "css-loader": "^6.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "json-loader": "^0.5.7",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.14.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.0",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@types/leaflet": "^1.9.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "framer-motion": "^10.0.0",
    "leaflet": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.0",
    "waffle-map-mesh-calculator-basic": "^2.3.3",
    "webpack-dev-server": "^5.2.2",
    "zustand": "^4.4.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js|jsx)"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  }
}
