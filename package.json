    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.0",
    "jest-junit": "^13.0.0",
    "json-loader": "^0.5.7",
    "prettier": "^2.0.0",
    "react-test-renderer": "^18.0.0",
