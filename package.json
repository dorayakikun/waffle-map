  "author": "dorayakikun <amakunai.dorayaki@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map.git"
  },
  "homepage": "https://dorayakikun.github.io/waffle-map/",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/react": "^4.1.7",
    "@types/jest": "^24.0.0",
    "@types/react-test-renderer": "^16.0.3",
    "@types/storybook__addon-links": "^3.3.3",
    "@types/storybook__react": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^1.0.0",
    "@typescript-eslint/parser": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "connect": "^3.6.6",
    "core-js": "^2.6.2",
    "del": "^3.0.0",
