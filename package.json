{
  "name": "waffle-map",
  "version": "1.21.0",
  "description": "Fantastic mesh renderer",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --hot --mode development",
    "fmt": "deno fmt -c deno.jsonc",
    "lint": "deno lint -c deno.jsonc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p . --noEmit"
  },
  "author": "dorayakikun <github@dorayakikun.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map.git"
  },
  "homepage": "https://dorayakikun.github.io/waffle-map/",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-test-renderer": "^18.3.0",
    "@vitest/coverage-v8": "^0.34.6",
    "babel-loader": "^10.0.0",
    "connect": "^3.7.0",
    "core-js": "^3.47.0",
    "css-loader": "^7.1.2",
    "jsdom": "^22.1.0",
    "json-loader": "^0.5.7",
    "react-test-renderer": "^18.3.1",
    "regenerator-runtime": "^0.14.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.1",
    "@types/react-helmet": "^6.1.11",
    "@types/react-redux": "^7.1.34",
    "@types/redux": "^3.6.0",
    "framer-motion": "^12.23.24",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-leaflet": "^4.2.1",
    "waffle-map-mesh-calculator-basic": "^2.3.3",
    "webpack-dev-server": "^5",
    "zustand": "^5.0.8"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js|jsx)"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  }
}
