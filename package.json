{
  "name": "waffle-map",
  "version": "2.0.0",
  "description": "Fantastic mesh renderer",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/generate-404.js dist",
    "preview": "vite preview",
    "fmt": "biome format --write .",
    "fmt:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:property": "vitest --run src/domain/__tests__/*.property.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "typecheck": "tsc -p . --noEmit"
  },
  "author": "dorayakikun <github@dorayakikun.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map.git"
  },
  "homepage": "https://dorayakikun.github.io/waffle-map/",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2",
    "@playwright/test": "^1.57.0",
    "@testing-library/react": "^16",
    "@vitejs/plugin-react": "^5",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.0",
    "connect": "^3.6.6",
    "fast-check": "^4.5.3",
    "jsdom": "^27",
    "typescript": "^5.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.0.0",
    "@emotion/react": "^11",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.562.0",
    "react": "^19",
    "react-dom": "^19",
    "react-leaflet": "^5",
    "waffle-map-mesh-calculator-basic": "github:dorayakikun/waffle-map-mesh-calculator-basic#ee66cc18785d308390b8ed1c2dd0d553ac93528a",
    "zustand": "^5.0.9"
  }
}
